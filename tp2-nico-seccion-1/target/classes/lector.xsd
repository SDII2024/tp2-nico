<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.webservicesoap.org/lector"
           xmlns:tns="http://www.webservicesoap.org/lector"
           elementFormDefault="qualified">

    <xs:element name="Lector">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="codigo" type="xs:int" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="LectorResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="producto" type="xs:string" />
                <xs:element name="precio" type="xs:string" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>